#include <iostream>
using namespace std;
void sortarr(int* arr,int n){
    //base case
    if(n==0 || n==1) return;
    
    //1st case solve
    for(int i=0;i<n-1;i++){
        if(arr[i]>arr[i+1]){
            swap(arr[i],arr[i+1]);
        }
    }
    //recursive call
    sortarr(arr, n-1);
    
}
int main() {
    int arr[5]={15,23,1,6,12};
    sortarr(arr,5);
    for(int i=0;i<5;i++){
        cout<<arr[i]<<" ";
    }

    return 0;
}